{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "array-bracket-spacing": [2, "always"],
    "comma-dangle": 0,
    "consistent-return": 1,
    "curly": 0,
    "eqeqeq": [2, "smart"],
    "max-len": 0,
    "no-confusing-arrow": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-shadow": 1,
    "no-use-before-define": 1,
    "prefer-template": 0,
    "react/prefer-es6-class": 0,
    "react/prop-types": 0,
    "react/jsx-space-before-closing": 0,
    "semi": [2, "never"]
  }
}
